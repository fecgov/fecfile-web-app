{
  "name": "fec-e-file",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-docker": "npm install && snyk protect && node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --port 80 --host 0.0.0.0 --configuration=local",
    "local": "ng serve --configuration=local",
    "local_memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --configuration=local",
    "local_aot": "ng serve --configuration=local --aot",
    "local_aot_memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --configuration=local --aot",
    "build-dev": "npm install && snyk protect && ng build --configuration=cloud.gov.dev",
    "build-stage": "npm install && snyk protect && ng build --configuration=cloud.gov.stage",
    "build-prod": "npm install && snyk protect && ng build --configuration=cloud.gov.prod",
    "build-local": "npm install && snyk protect && ng build --configuration=local",
    "test": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng test",
    "e2e": "ng e2e",
    "json-server": "nodemon ./server/server.js",
    "lint": "sh ./build-scripts/lint.sh",
    "license-report": "./node_modules/.bin/license-checker",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prettier:check:ci": "./node_modules/.bin/prettier --check .",
    "prettier:fix": "./node_modules/.bin/prettier --write ."
  },
  "private": true,
