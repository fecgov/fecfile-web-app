{
    "name": "fec-e-file",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start-docker": "npm install && snyk protect && node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --port 80 --host 0.0.0.0 --configuration=local",
        "local": "ng serve --configuration=local",
        "local_memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --configuration=local",
        "local_aot": "ng serve --configuration=local --aot",
        "local_aot_memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --configuration=local --aot",
        "build-dev": "npm install && snyk protect && ng build --configuration=cloud.gov.dev",
        "build-stage": "npm install && snyk protect && ng build --configuration=cloud.gov.stage",
        "build-prod": "npm install && snyk protect && ng build --configuration=cloud.gov.prod",
        "build-local": "npm install && snyk protect && ng build --configuration=local",
        "test": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng test",
        "e2e": "ng e2e",
        "json-server": "nodemon ./server/server.js",
        "lint": "sh ./build-scripts/lint.sh",
        "license-report": "./node_modules/.bin/license-checker",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.0",
        "@angular/cdk": "^13.2.6",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/forms": "~13.2.0",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "@ngrx/effects": "^13.0.2",
        "@ngrx/store": "^13.0.2",
        "@popperjs/core": "^2.10.2",
        "bootstrap": "^5.1.3",
        "class-transformer": "^0.5.1",
        "fecfile-validate": "https://github.com/fecgov/fecfile-validate#6e6ea5905f823b125ea548bebb0d05c92642d849",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.21",
        "luxon": "^2.3.2",
        "ngrx-store-localstorage": "^13.0.0",
        "ngx-cookie-service": "^13.1.2",
        "ngx-logger": "^5.0.7",
        "primeflex": "^3.1.3",
        "primeicons": "^5.0.0",
        "primeng": "^13.2.1",
        "rxjs": "^7.5.4",
        "tslib": "^2.3.1",
        "zone.js": "^0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.2",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "~13.2.2",
        "@angular/compiler-cli": "~13.2.0",
        "@cypress/schematic": "^1.6.0",
        "@types/jasmine": "~3.10.0",
        "@types/lodash": "^4.14.178",
        "@types/luxon": "^2.3.1",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "cypress": "9.5.3",
        "eslint": "^8.2.0",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "karma-spec-reporter": "^0.0.33",
        "license-checker": "^25.0.1",
