 {
    "extends": "../tsconfig.json",
    "compilerOptions": {
    "sourceMap": false,
    "target": "ES2024",
    "module": "commonjs",
    "lib": ["ES2024", "dom"],
    "strict": true,
    "esModuleInterop": true,
    "types": ["cypress", "node", "cypress-terminal-report", "cypress-mochawesome-reporter"],
    "moduleResolution": "node"
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts",
    "./support/e2e.ts",
    "./bs-cypress.d.ts"
  ]
}